{
    "name": "mrstebo/laravel-socialite-ekm",
    "description": "EKM OAuth2 Provider for Laravel Socialite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Atkinson",
            "email": "mrstebouk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "laravel/socialite": "^4.0",
        "lcobucci/jwt": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Mrstebo\\LaravelSocialiteEKM\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrstebo\\LaravelSocialiteEKM\\Provider"
            ]
        }
    }
}
